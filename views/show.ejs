            </a>
            <a class="level-item">
              <span class="icon is-small"><i class="fad fa-heart"></i></span>
            </a>
          </div>
        </nav>
      </div>
      <div class="media-right">
        <% if(user){ %>
        <button class="delete"></button>
        <% }%>
      </div>
    </article>



    <% }%>





